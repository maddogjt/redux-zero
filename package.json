{
  "name": "predux",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "--exports": {
    ".": "./lib/index.js",
    "./middleware": "./lib/middleware/index.js",
    "./utils": "./lib/utils/index.js",
    "./devtools": "./lib/devtools/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf lib dist es coverage types",
    "--compile": "rimraf lib && tsc --pretty",
    "compile": "tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.cjs.spec.json ./src/tsconfig.esm.json ./src/tsconfig.types.json ./src/tsconfig.types.spec.json",
    "compile:mini": "tsc -b ./src/tsconfig.esm.json",
    "compile:watch": "tsc --pretty --watch",
    "build:clean": "rimraf ./dist",
    "build:package": "npm-run-all build:clean compile && node ./tools/generate-alias.js",
    "lint": "eslint --ext .ts --ext .tsx src",
    "ignore": " 'src/**/*.@(ts|tsx)'",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"config/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "author": "Jason Tranchida <projects@projects.land>",
  "files": [
    "dist/cjs/**/!(*.tsbuildinfo)",
    "dist/esm/**/!(*.tsbuildinfo)",
    "dist/types/**/!(*.tsbuildinfo)",
    "util",
    "middleware",
    "preact",
    "src",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "LICENSE.txt",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maddogjt/predux"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/**/*.spec.(ts|tsx)"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "7.14.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "7.14.6",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "3.1.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "fs-extra": "10.0.0",
    "jest": "27.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "tslib": "2.3.0",
    "typescript": "4.3.4"
  },
  "peerDependencies": {
    "preact": "10.x"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    }
  },
  "keywords": [
    "redux",
    "flux",
    "state",
    "predictable",
    "functional",
    "immutable",
    "elm",
    "lightweight",
    "light",
    "small",
    "simple"
  ]
}
